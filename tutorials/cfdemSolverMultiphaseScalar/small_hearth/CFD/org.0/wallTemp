FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      wallTemp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
  atmosphere
    {
        type            zeroGradient;
    }
  walls
    {
        type            fixedValue;
	value           uniform 1473.15; 
    }
  outlet
    {
        type            zeroGradient;
    }
  inlet
    {
        type            zeroGradient;
    }
}


