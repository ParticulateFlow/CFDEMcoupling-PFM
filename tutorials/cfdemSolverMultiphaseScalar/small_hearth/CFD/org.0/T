FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1623.15;

boundaryField
{
  atmosphere
    {
        type            zeroGradient;
    }
  walls
    {
        type            zeroGradient;
    }
  outlet
    {
        type            zeroGradient;
    }
  inlet
    {
	type		fixedValue;
	value 		uniform 1823.15;
    }
}


