FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     cfdemSolverPimple;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         20;

deltaT          0.0002;

writeControl    adjustableRunTime;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo 0.8;


functions
{
   fieldAverage1
    {
        type            fieldAverage;
        functionObjectLibs ( "libfieldFunctionObjects.so" );
        writeControl   writeTime;
        fields
        (
            Us
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
                 window      18.0;
            }


            voidfraction
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
                 window      18.0;
            }

            p
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
                 window      18.0;
            }
         );
       }
}
