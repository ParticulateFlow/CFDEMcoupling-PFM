FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Ksl;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            cyclic;
    }

    top
    {
        type            cyclic;
    }

    lr1
    {
        type            cyclic;
    }

    lr2
    {
        type            cyclic;
    }

    vh1
    {
        type            cyclic;
    }

    vh2
    {
        type            cyclic;
    }
}

