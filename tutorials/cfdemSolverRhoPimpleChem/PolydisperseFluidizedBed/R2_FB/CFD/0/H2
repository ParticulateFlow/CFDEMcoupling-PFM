{
    side-walls
    {
        type            zeroGradient;
    }

    inlet
    {
	type            fixedValue;
	value		uniform 0.0116562393;
    }

    outlet
    {
        type            zeroGradient;
/*        type            inletOutlet;  
	value		$internalField;
        inletValue	uniform 0.; */
    }
}


