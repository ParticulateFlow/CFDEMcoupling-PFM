FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.003;

boundaryField
{
    top
    {
        type            slip;
    }
    bottom
    {
        type            slip;
    }
    side-walls
    {
        type            slip;
    }
    inlet
    {
        type            zeroGradient;
        // If turbulence is present
        /*         
         * type            compressible::turbulentMixingLengthDissipationRateInlet;
         * mixingLength    0.005;
         * value           uniform 200;
        */
    }
    outlet
    {
        type            zeroGradient;
    }
}


