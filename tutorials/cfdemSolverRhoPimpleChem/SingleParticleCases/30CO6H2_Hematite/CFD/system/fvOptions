FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
limitedTemperature
{
    type        limitTemperature;
    active      yes;
    limitTemperatureCoeffs
    {
        active          yes;
        selectionMode   all;
        Tmin     1200; // OF4
        Tmax     2500; // OF4
        min      $Tmin; // OF5,OF6
        max      $Tmax; // OF5,OF6
    }
}

