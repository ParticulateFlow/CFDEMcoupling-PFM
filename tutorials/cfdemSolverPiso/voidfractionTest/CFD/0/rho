FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 10;

boundaryField
{
    "(inlet|outlet|walls)"   
    {
        type            zeroGradient;
        value           uniform 10;
    }
}

