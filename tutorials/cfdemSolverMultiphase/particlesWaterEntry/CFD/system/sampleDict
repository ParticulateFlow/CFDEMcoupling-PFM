FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

sets
(
    data
    {
        type    uniform;
        axis    z;
        start   (0.025 0.025 0.2);
        end     (0.025 0.025 0);
        nPoints 1000;
    }
);

fields          (alpha.air);

