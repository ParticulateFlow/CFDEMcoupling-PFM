FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            uniformFixedValue;
        uniformValue    constant (0 0 0);
        value           uniform (0 0 0);
    }
    inlet
    {
        type            uniformFixedValue;
        uniformValue    table
        
2
(
(0 (0 0 0))
(1.0 (0 0 0.9))
)
;
        value           uniform (0 0 0.9);
    }
    outlet
    {
        type            zeroGradient;
    }
}


