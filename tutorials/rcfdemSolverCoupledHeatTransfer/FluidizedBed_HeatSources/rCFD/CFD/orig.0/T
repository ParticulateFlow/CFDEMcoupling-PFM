FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 324;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    inlet
    {
        type            uniformFixedValue;
        uniformValue    constant 324;
        value           uniform 324;
    }
    outlet
    {
        type            zeroGradient;
    }
}


