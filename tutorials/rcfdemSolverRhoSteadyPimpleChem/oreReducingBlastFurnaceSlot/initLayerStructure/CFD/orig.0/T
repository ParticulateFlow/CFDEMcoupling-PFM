FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293;

boundaryField
{
    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    wall
    {
        type            fixedValue;
        value		uniform 500;
    }

    inner
    {
        type            zeroGradient;
    }
    inletLeft
    {
        type            fixedValue;
        value		uniform 2600;
    }
    inletRight
    {
        type            fixedValue;
        value		uniform 2600;
    }
}

