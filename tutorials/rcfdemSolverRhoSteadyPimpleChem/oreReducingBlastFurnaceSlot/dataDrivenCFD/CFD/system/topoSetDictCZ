FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
actions
(
 {
    name    aboveCZ;
    type    cellSet;
    action  new;
    source fieldToCell;
    sourceInfo
    {
        field       "partTempRef";
        min         1675;
        max         10000;
    }
  }

  {
    name    aboveCZ;
    type    cellSet;
    action  add;
    source  boxToCell;
    sourceInfo
    {
      box (-8 -1 -2) (8 1 3);
    }
  }

  {
    name    aboveCZ;
    type    cellSet;
    action  invert;
  }
);

