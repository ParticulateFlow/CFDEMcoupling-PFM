FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.70;

boundaryField
{
    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    wall
    {
        type            zeroGradient;
    }

    inner
    {
        type            zeroGradient;
    }
    inletLeft
    {
        type            fixedValue;
        value           uniform 0.70;
    }
    inletRight
    {
        type            fixedValue;
        value           uniform 0.70;
    }

}


