FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      probesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fields
(
    voidfraction
    voidfractionMean
    voidfractionMean_w1
    rho
    p
    Us
    UsMean
    UsMean_w1
    U
);

writeControl   timeStep;
writeInterval  40;


// Locations to be probed.
probeLocations
(
    (0.001 0.001 -1.0)
    (0.001 0.001 0.0)
    (0.001 0.001 1.0)
    (0.001 0.001 2.0)
    (0.001 0.001 3.0)
    (0.001 0.001 4.0)
    (0.001 0.001 5.0)
    (0.001 0.001 6.0)
    (0.001 0.001 7.0)
    (0.001 0.001 8.0)
    (0.001 0.001 9.0)
    (0.001 0.001 10.0)
    (0.001 0.001 12.0)
    (0.001 0.001 14.0)
    (0.001 0.001 16.0)
    (0.001 0.001 18.0)
    (0.001 0.001 20.0)
    (0.001 0.001 22.0)
    (0.001 0.001 24.0)
    (0.001 0.001 26.0)
);

