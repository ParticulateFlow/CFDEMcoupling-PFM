FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      partEffThermCondField;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    walls
    {
        // fixed heat loss, put all in gas phase
        type            fixedValue;
        value           uniform 0.0;
    }

    inlet
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
