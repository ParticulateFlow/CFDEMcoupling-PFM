FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      phiIB;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet      
    {
        type            zeroGradient;
    }

    channelWall      
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }
}

