FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    walls
    {
       // type            zeroGradient;
       type        fixedFluxPressure; 
    }

    bottom
    {
       // type            zeroGradient;
       type        fixedFluxPressure; 
    }

    inlet
    {
        //type            zeroGradient;
        type        fixedFluxExtrapolatedPressure;
       //type        fixedFluxPressure;
     //   value       uniform 1.03e5;
    }

    outlet
    {
        type            uniformFixedValue;
        uniformValue    constant 1e5;
    }
}

