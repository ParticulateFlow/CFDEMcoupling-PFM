probeLocations
(
	(0.0 0.0 0.03)
        (0.0 0.0 0.06)
        (0.0 0.0 0.09)
	(0.035 0.0 0.03)
	(-0.035 0.0 0.03)
	(0.02 0.0 0.001)
	(-0.02 0.0 0.001)

);

