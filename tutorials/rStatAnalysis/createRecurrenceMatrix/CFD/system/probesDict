FoamFile
{
	version		2.0;
	format		ascii;
	class		dictionary;
	object		probesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



fields
(
    voidfraction
    UsRec
    URec
    pRec
    rhoRec
    voidfractionRec
    addSource
);

outputControl   timeStep; //outputTime;

outputInterval  40;


